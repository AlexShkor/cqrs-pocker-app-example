@model System.Linq.IOrderedEnumerable<AKQ.Domain.Services.UserLogDocument>

<div class="container">
<table class="table table-striped table-condensed">
    <tbody>
        @foreach(var user in Model)
        {
            <tr>
                <td>@user.UserName</td>
                <td>@user.Date</td>
                <td>@user.Action</td>
            </tr>
        }
    </tbody>
</table>
    </div>