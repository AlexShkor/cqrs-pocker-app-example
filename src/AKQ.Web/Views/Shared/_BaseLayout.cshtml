@using System.Web.Optimization
@using AKQ.Web.App_Start

<!DOCTYPE html>
<html ng-app>
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
        <title>AKQbridge | @ViewBag.Title</title>

        <link rel="SHORTCUT ICON" href="~/Assets/app/css/img/favicon.ico" />
        @Styles.Render("~/content/css")
        @Scripts.Render("~/scripts/jquery-2.0.3.min.js")
        <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.0.1/angular.min.js" type="text/javascript"></script>
        <script src="/assets/app/js/app.js" type="text/javascript"></script>

        @Scripts.Render("~/scripts/jquery.signalR-1.1.2.min.js")
        @Scripts.Render("~/signalr/hubs")
    </head>
<body class="landing-layout">
    <div id="globalLoader" 
         style="display: none;" >
        <div class="circle"></div>
        <div class="circle1"></div>
    </div>
    
@Html.Partial("Navigation")
    <div id="main">@RenderBody()</div>
</body>
</html>


